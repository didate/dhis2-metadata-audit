            <column name="created" type="date"/>
            <column name="name" type="string"/>
            <column name="short_name" type="string"/>
            <column name="generated" type="boolean"/>
            <column name="value_type" type="string"/>
            <column name="confidential" type="boolean"/>
            <column name="display_form_name" type="string"/>
            <column name="uniquee" type="boolean"/>
            <column name="dimension_item_type" type="string"/>
            <column name="aggregation_type" type="string"/>
            <column name="display_in_list_no_program" type="boolean"/>
            <column name="display_name" type="string"/>
            <column name="patterne" type="string"/>
            <column name="skip_synchronization" type="boolean"/>
            <column name="display_short_name" type="string"/>
            <column name="period_offset" type="numeric"/>
            <column name="display_on_visit_schedule" type="boolean"/>
            <column name="form_name" type="string"/>
            <column name="orgunit_scope" type="boolean"/>
            <column name="dimension_item" type="string"/>
            <column name="inherit" type="boolean"/>
            <column name="option_set_value" type="boolean"/>
            <column name="track" type="string"/>
            <column name="created_by_id" type="string"/>
            <column name="last_updated_by_id" type="string"/>
            <!-- jhipster-needle-liquibase-add-loadcolumn - JHipster (and/or extensions) can add load columns here -->
        </loadData>
    </changeSet>
</databaseChangeLog>
