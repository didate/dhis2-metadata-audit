        <addForeignKeyConstraint baseColumnNames="program_id"
                                 baseTableName="program_rule"
                                 constraintName="fk_program_rule__program_id"
                                 referencedColumnNames="id"
                                 referencedTableName="program"
                                 />
    </changeSet>
</databaseChangeLog>
